<%- contentFor('content') %>
<div class= "container">
<div>
      <h1>All Products</h1>
      <hr>

<table class="table table-inverse col-md-10">
<thead>
<tr>
  <% thead.forEach(function(item, index){ %>
  <th><%- item %></th>
  <% }) %>
    
</tr>
</thead>
    <tbody>
    <% data.forEach(function(item,index){ %>
      <tr id= '<%- slug %>'>
        <td><%- item.name %></td>
        <td><%- item.brand %></td>
        <td><%- item.category %></td>
        <td><%- item.description %></td>
        <td><%- item.slug %></td>
        <td>
        <a href="/admin/edit/<%- item.slug %>"><span class="glyphicon glyphicon-check"></span></a>
        <a href="/admin/edit/<%- item.slug %>/delete"><span class="glyphicon glyphicon-remove-circle"></span></a>
        </td>
      </tr> 
    <% }) %>
            

    </tbody>
</table>

  </div>

</div>

